    "@angular/compiler-cli": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@types/datatables.net": "~1.10.19",
    "@types/jasmine": "~3.6.1",
    "@types/jquery": "~3.5.4",
    "@types/node": "~14.14.6",
    "canonical-path": "1.0.0",
    "codelyzer": "^6.0.1",
    "concat-cli": "~4.0.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "datatables.net": "^1.10.22",
    "jasmine-core": "~3.6.0",
    "jquery": "^3.4.1",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "~4.0.1",
    "protractor": "~7.0.0",
    "rimraf": "~3.0.2",
    "rollup": "~2.33.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rxjs": "^6.6.3",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": ">=8.9.0"
  },
